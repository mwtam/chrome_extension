{
    "manifest_version": 3,
    "name": "Hello Extensions of the world!",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "hello.html",
        "default_icon": "entrances.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://example.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}